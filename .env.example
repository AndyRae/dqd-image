# Data Quality Dashboard â€“ copy to .env and fill in.
# Image includes PostgreSQL JDBC driver (same as downloadJdbcDrivers("postgresql")).
# For other DBs: set DATABASECONNECTOR_JAR_FOLDER and use downloadJdbcDrivers("redshift"), etc., then mount that folder as ./jdbc. See https://ohdsi.github.io/DatabaseConnector/reference/jdbcDrivers.html

# Database connection
DQD_DBMS=postgresql
DQD_SERVER=host.docker.internal
DQD_PORT=5432
DQD_USER=
DQD_PASSWORD=

# OMOP CDM schema and results
DQD_CDM_SCHEMA=cdm
DQD_RESULTS_SCHEMA=results
DQD_CDM_SOURCE_NAME=My CDM
DQD_CDM_VERSION=5.4

# Output (container path; host ./output is mounted there)
DQD_OUTPUT_FOLDER=/output
DQD_OUTPUT_FILE=dqd_result.json

# Optional
DQD_NUM_THREADS=1
DQD_WRITE_TO_TABLE=TRUE
DQD_TABLE_NAME=dqd_results
