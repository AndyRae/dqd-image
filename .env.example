# Data Quality Dashboard â€“ copy to .env and fill in.
# Image includes PostgreSQL JDBC driver; for other DBs mount a ./jdbc volume with the driver jar(s).

# Database connection
DQD_DBMS=postgresql
DQD_SERVER=host.docker.internal
DQD_PORT=5432
DQD_USER=
DQD_PASSWORD=

# OMOP CDM schema and results
DQD_CDM_SCHEMA=cdm
DQD_RESULTS_SCHEMA=results
DQD_CDM_SOURCE_NAME=My CDM
DQD_CDM_VERSION=5.4

# Output (container path; host ./output is mounted there)
DQD_OUTPUT_FOLDER=/output
DQD_OUTPUT_FILE=dqd_result.json

# Optional
DQD_NUM_THREADS=1
DQD_WRITE_TO_TABLE=TRUE
DQD_TABLE_NAME=dqd_results
